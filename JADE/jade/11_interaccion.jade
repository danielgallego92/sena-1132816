doctype
html(lang="es")
  head
    meta(charset="UTF-8")
    title interaccion
    body
      ul
        each val in [1, 2, 3, 4, 5]
          li= val
      br
      br
      ul
        each val, index in ['zero', 'one', 'two']
          li= index + ': ' + val
      br
      br
      ul
        each val, index in {1:'one',2:'two',3:'three'}
          li= index + ': ' + val
      br
      br
      - var values = [];
      ul
        each val in values.length ? values : ['There are no values']
          li= val
      